{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: yarn dev",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev"],
      "port": 9229,
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "timeout": 10000,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
